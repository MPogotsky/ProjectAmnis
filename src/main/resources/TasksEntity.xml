<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.amnis.model.TasksEntity">
        <table name="tasks" schema="server_database" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="userId">
                <column name="user_id"/>
            </basic>
            <basic name="taskStatus">
                <column name="task_status"/>
            </basic>
            <basic name="estimatedTime">
                <column name="estimated_time"/>
            </basic>
            <basic name="points">
                <column name="points"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
