<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.amnis.model.UsersEntity">
        <table name="users" schema="server_database" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="login">
                <column name="login"/>
            </basic>
            <basic name="token">
                <column name="token"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
